{
  "name": "backend-start",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "gql-gen": "graphql-codegen",
    "build": "gql-gen && rimraf dist && tsc && tsc-alias && copyfiles -u 1  src/**/*.graphql dist",
    "dev": " cd src && tsx index.ts",
    "start": "cd dist && cross-env NODE_ENV=production node --experimental-specifier-resolution=node index.js",
    "knex:make": "cd src && tsx ./utils/knexMigration.ts knex:make",
    "knex:latest": "cd src && tsx ./utils/knexMigration.ts knex:latest",
    "knex:rollback": "cd src && tsx ./utils/knexMigration.ts knex:rollback",
    "knex:rollbackAll": "cd src && tsx ./utils/knexMigration.ts knex:rollbackAll",
    "knex:up": "cd src && tsx ./utils/knexMigration.ts knex:up",
    "knex:down": "cd src && tsx ./utils/knexMigration.ts knex:down",
    "knex:version": "cd src && tsx ./utils/knexMigration.ts knex:currentVersion",
    "knex:list": "cd src && tsx ./utils/knexMigration.ts knex:list",
    "knex:unlock": "cd src && tsx ./utils/knexMigration.ts knex:unlock",
    "discord:initCommands": "cd src && tsx ./init/initDiscordCommands.ts",
    "redis:initData": "cd src && tsx ./init/initRedisData.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jang2162/jang2162-backend-start.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/jang2162/jang2162-backend-start/issues"
  },
  "homepage": "https://github.com/jang2162/jang2162-backend-start#readme",
  "dependencies": {
    "@apollo/server": "^4.8.0",
    "@graphql-tools/load-files": "^7.0.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "camelcase": "^7.0.1",
    "clone": "^2.1.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dataloader": "^2.1.0",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "graphql": "^16.6.0",
    "graphql-tools": "^8.3.14",
    "js-base64": "^3.7.5",
    "jsonwebtoken": "^9.0.1",
    "knex": "^2.5.1",
    "pg": "^8.11.1",
    "redis": "^4.6.7",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.7.0",
    "uuid": "^9.0.0",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^5.0.0",
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-graphql-files-modules": "^2.2.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/clone": "^2.1.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
